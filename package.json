{
  "name": "ecommerce-admin",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "HOST=$(grep HOST .env | cut -d '=' -f2) npm run start-prod",
    "start-prod": "vite preview  --host $HOST",
